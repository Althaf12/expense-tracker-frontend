# Expense Tracker Frontend - Environment Variables
# Copy this file to .env.local and update values for your environment

# Backend API base URL for this expense tracker subdomain
VITE_API_BASE=http://localhost:8081/api

# Auth service base URL (SSO server)
# - Development: http://localhost:8080
# - Production: https://auth.eternivity.com
VITE_AUTH_BASE_URL=http://localhost:8080

# This app's base URL (for logout redirect)
# - Development: http://localhost:5176
# - Production: https://expensetracker.eternivity.com
VITE_APP_BASE_URL=http://localhost:5176

# Main Eternivity site URL (for registration redirect)
# - Development: http://localhost:5175
# - Production: https://eternivity.com
VITE_MAIN_SITE_URL=http://localhost:5175